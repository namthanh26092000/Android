<script>
var selectCallback = function(variant, selector) {
	if (variant && variant.available) {
		if(variant.featured_image != null)
		{
			$(".slide_thumb_img li.product-thumb img[data-image='"+Haravan.resizeImage(variant.featured_image.src,'master')+"']").click().parents('li').addClass('active');
		}
		if (variant.sku != null ){
			jQuery('.sku').html('<strong>Mã sản phẩm: </strong>'+variant.sku);
		}else{
			jQuery('.sku').html('<strong>Mã sản phẩm: </strong>Chưa rõ');
		}
		jQuery('.btn-addCart').removeClass('hidden');
		jQuery('.btn-Soldout').addClass('hidden');
		jQuery('.soldout').remove();
		if(variant.price < variant.compare_at_price){
			//jQuery('#price-preview').html("<span>" + Haravan.formatMoney(variant.price, "{{amount}}₫") + "</span><del>" + Haravan.formatMoney(variant.compare_at_price, "{{amount}}₫") + "</del>");
			jQuery('.price.detail-info-entry .current').html(Haravan.formatMoney(variant.price, "{{amount}}₫"));
			jQuery('.price.detail-info-entry .prev').remove();
			jQuery('.price.detail-info-entry .current').after('<div class="prev">'+Haravan.formatMoney(variant.compare_at_price, "{{amount}}₫")+'</div>')
			var pro_sold = variant.price ;
			var pro_comp = variant.compare_at_price / 100;
			var sale = 100 - (pro_sold / pro_comp) ;
			var kq_sale = Math.round(sale);
			jQuery('.sale').html('Giảm: '+kq_sale+"%");
		} else {
			jQuery('.price.detail-info-entry .current').html(Haravan.formatMoney(variant.price, "{{amount}}₫"));
			jQuery('.price.detail-info-entry .prev').remove();
			jQuery('.sale').remove();
		}
		check_variant = true;
	} else {
		jQuery('.btn-Soldout').removeClass('hidden');
		jQuery('.btn-addCart').addClass('hidden');
		jQuery('.product-preview-box').append('<div class="soldout insLabel"><span>Hết hàng</span></div>');
		check_variant = false;
	}
	return check_variant;
};

